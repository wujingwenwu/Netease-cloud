.sign{
  width: 100%;
  height: 100rpx;
  background: red;
  color: white;
  font-size: 36rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.code{
  width: 200rpx;
  height: 80rpx;
  background: green;
  color: white;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  z-index: 999;
  right: 20rpx;
  top: 275rpx;
}
.danger{
  display: flex;
  flex-direction: column;
  align-items: center;
}